packages:
  yum:
    libcurl-devel: []
container_commands:
  collectstatic:
    command: "python manage.py collectstatic --noinput --settings ITUscheduler.settings.production"
  celery:
    command: "python manage.py celery --settings ITUscheduler.settings.production"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: ITUscheduler/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "ITUscheduler.settings.production"