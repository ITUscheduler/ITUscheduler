container_commands:
  migrate:
    command: "python manage.py migrate --noinput --settings ITUscheduler.settings.production"
    leader_only: true
  collectstatic:
    command: "python manage.py collectstatic --noinput --settings ITUscheduler.settings.production"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: ITUscheduler/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "ITUscheduler.settings.production"
  aws:elbv2:listener:443:
    ListenerEnabled: "true"
    Protocol: HTTPS
    SSLCertificateArns: arn:aws:acm:eu-central-1:048826950968:certificate/af94be66-e024-41dc-8dbd-81c6ff3aba46
  aws:autoscaling:launchconfiguration:
    SecurityGroups: ITUscheduler